<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:import field="edu.mit.scansite.shared.event.NavigationEvent.PageId.*" />
	<g:HTMLPanel>
		<ul>
			<li id="homeLink">
				<g:Hyperlink ui:field="homeButton" text="Home"
					targetHistoryToken="{HOME.getId}" />
			</li>
			<li id="newsLink">
				<g:Hyperlink ui:field="newsButton" text="News"
					targetHistoryToken="{NEWS.getId}" />
			</li>
			<li>
				<h3>Features</h3>
				<ul>
					<li id="scanProteinLink">
						<g:Hyperlink ui:field="scanProteinButton" text="Scan Protein for Motifs"
							targetHistoryToken="{FEATURE_SCAN_PROTEIN.getId}" />
					</li>
					<li id="scanDbLink">
						<g:Hyperlink ui:field="scanDbButton"
							text="Search a Sequence Database for Motifs" targetHistoryToken="{FEATURE_SCAN_DB.getId}" />
					</li>
					<li id="scanSeqLink">
						<g:Hyperlink ui:field="scanSeqButton" text="Find Sequence Match"
							targetHistoryToken="{FEATURE_SCAN_SEQ.getId}" />
					</li>
					<li id="scanOrthologsLink">
						<g:Hyperlink ui:field="scanOrthologsButton" text="Scan Orthologs"
							targetHistoryToken="{FEATURE_SCAN_ORTHOLOGS.getId}" />
					</li>
					<li id="predictLocalizationLink">
						<g:Hyperlink ui:field="predictLocalizationButton"
							text="Predict Localization" targetHistoryToken="{FEATURE_PREDICT_LOCALIZATION.getId}" />
					</li>
					<li id="calcMolWeightLink">
						<g:Hyperlink ui:field="calcMolWeightButton"
							text="Calculate Molecular Weight and pI" targetHistoryToken="{FEATURE_CALC_MOLWEIGHT.getId}" />
					</li>
					<li id="calcCompositionLink">
						<g:Hyperlink ui:field="calcCompositionButton"
							text="Calculate Amino Acid Composition" targetHistoryToken="{FEATURE_CALC_COMPOSITION.getId}" />
					</li>
				</ul>
			</li>
			<li id="motifsLink">
				<g:Hyperlink ui:field="motifsButton" text="Databases and Motifs"
					targetHistoryToken="{MOTIFS.getId}" />
			</li>
			<li id="tutorialLink">
				<h3>Tutorials</h3>
				<g:Hyperlink ui:field="tutorialButton" text="Tutorial Overview"
					targetHistoryToken="{TUTORIAL.getId}" />
				<ul>
					<li id="PScanTutorialLink">
						<g:Hyperlink ui:field="tutorialPScanButton" text="Protein Scan"
									 targetHistoryToken="{PSCAN_TUTORIAL.getId}" />
					</li>
					<li id="dbSearchTutorialLink">
						<g:Hyperlink ui:field="tutorialDBSearchButton" text="Database Search"
									 targetHistoryToken="{DBSEARCH_TUTORIAL.getId}" />
					</li>
					<li id="SeqMatchTutorialLink">
						<g:Hyperlink ui:field="tutorialSeqMatchButton" text="Sequence Match"
									 targetHistoryToken="{SEQMATCH_TUTORIAL.getId}" />
					</li>
				</ul>
			</li>
			<li id="adminLink" style="display: none">
				<h3>Administration Area</h3>
				<g:Hyperlink ui:field="adminButton" text="Hub"
					targetHistoryToken="{ADMIN.getId}" />
				<ul id="adminSubnav" style="display: none;">
					<li id="motifMgmtLink" style="display: none;">
						<g:Hyperlink ui:field="adminMotifButton" text="Manage Motifs"
							targetHistoryToken="{ADMIN_MOTIF.getId}" />
					</li>
					<li id="motifGroupMgmtLink" style="display: none;">
						<g:Hyperlink ui:field="adminMotifGroupButton" text="Manage Motif Groups"
							targetHistoryToken="{ADMIN_MOTIF_GROUP.getId}" />
					</li>
					<li id="newsMgmtLink" style="display: none;">
						<g:Hyperlink ui:field="adminNewsButton" text="Manage News"
							targetHistoryToken="{ADMIN_NEWS.getId}" />
					</li>
					<li id="userMgmtLink" style="display: none;">
						<g:Hyperlink ui:field="adminUserButton" text="Manage Users"
							targetHistoryToken="{ADMIN_USER.getId}" />
					</li>
				</ul>
			</li>
		</ul>
	</g:HTMLPanel>
</ui:UiBinder> 