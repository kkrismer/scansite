<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:wg="urn:import:edu.mit.scansite.client.ui.widgets.general">
	<g:HTMLPanel>
		<h2>Administrator and Collaborator Area</h2>
		<fieldset>
			<legend>Sign in</legend>
			<ol>
				<li>
					<label for="emailAddressTextBoxId">
						E-mail address
					</label>
					<g:TextBox ui:field="emailAddressTextBox" />
				</li>
				<li>
					<label for="passwordTextBoxId">
						Password
					</label>
					<g:PasswordTextBox ui:field="passwordTextBox" />
				</li>
				<li>
					<g:SubmitButton ui:field="loginButton">Login</g:SubmitButton>
					<wg:MessageDisplayWidget ui:field="messageDisplayWidget" messageDisplayWidgetID="0" />
				</li>
			</ol>
		</fieldset>
	</g:HTMLPanel>
</ui:UiBinder> 